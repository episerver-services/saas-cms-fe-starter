{
  "name": "saas-cms-fe-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next/cache",
    "prepare": "husky install",
    "dev": "pnpm clean && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ui": "jest --selectProjects jsdom-ui",
    "test:api": "jest --selectProjects node-routes",
    "bdd": "cucumber-js --require-module ts-node/register --require features/step-definitions/**/*.ts",
    "test:features": "cucumber-js features/**/*.feature --require-module ts-node/register --require features/step-definitions/**/*.ts --require features/support/**/*.ts",
    "test:bdd": "start-server-and-test dev http://localhost:3000 bdd",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:headed": "pnpm exec playwright test --headed --debug",
    "codegen": "cross-env graphql-codegen --config codegen.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.525.0",
    "negotiator": "^1.0.0",
    "next": "15.3.3",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@cucumber/cucumber": "^11.3.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.2",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-document-nodes": "^4.0.16",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@playwright/test": "^1.54.1",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-themes": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/nextjs-vite": "^9.1.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chai": "^5.2.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^30.0.0",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19",
    "@types/react-test-renderer": "^19.1.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "chai": "^5.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-plugin-storybook": "^9.1.1",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "openapi-to-graphql": "^3.0.7",
    "playwright": "^1.54.1",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "react-test-renderer": "19.1.0",
    "start-server-and-test": "^2.0.12",
    "storybook": "^9.1.1",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
